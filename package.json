{
  "name": "monPlanR",
  "version": "0.1.1",
  "description": "A course planning tool designed by Monash students for Monash students.",
  "main": "./app/index.jsx",
  "dependencies": {
    "axios": "^0.14.0",
    "babel": "^6.5.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "if-env": "^1.0.0",
    "lodash": "^4.17.2",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-responsive": "^1.2.5",
    "react-router": "^2.8.1",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.61.9"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "scripts": {
    "production": "webpack --progress -p",
    "dev": "webpack-dev-server --host 0.0.0.0 --inline --content-base app/ --history-api-fallback",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint --ext .jsx --ext .js app",
    "test:unit": "mocha --compilers babel-core/register tests/*.spec.jsx",
    "fix": "eslint --ext .jsx --ext .js app --fix",
    "jsdoc": "jsdoc -c ./jsdoc.conf.json -d doc -r",
    "start:prod": "webpack --progress -p && node server.js",
    "start": "if-env NODE_ENV=\"production\" && npm run start:prod || npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MonashUnitPlanner/monPlanR.git"
  },
  "author": "eSolutions (Monash University)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MonashUnitPlanner/monPlanR/issues"
  },
  "homepage": "https://github.com/MonashUnitPlanner/monPlanR#readme"
}
